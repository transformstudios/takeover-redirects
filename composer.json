{
    "name": "transformstudios/takeover-redirects",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.59 || ^9.9",
        "statamic/cms": "^3.2",
        "edalzell/forma": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "nunomaduro/collision": "^5.0",
        "spatie/test-time": "^1.2",
        "spatie/laravel-ray": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "TransformStudios\\TakeoverRedirects\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TransformStudios\\TakeoverRedirects\\Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Takeover Redirects",
            "description": "Takeover Redirects addon"
        },
        "laravel": {
            "providers": [
                "TransformStudios\\TakeoverRedirects\\ServiceProvider"
            ]
        }
    }
}
